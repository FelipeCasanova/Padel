<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="es" ng-app="padelApp">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <link href="@Url.Content(@Links.Content.bootstrap.css.bootstrap_min_css)" rel="stylesheet" />
    <link href="@Url.Content(@Links.Content.fontawesome.css.font_awesome_css)" rel="stylesheet" type="text/css" />
    <link href="@Url.Content(@Links.Content.admin.grid.ng_grid_css)" rel="stylesheet" type="text/css" />
    <link href="@Url.Content(@Links.Content.admin.style_css)" rel="stylesheet" type="text/css" />
    <title>@ViewBag.Title</title>
</head>
<body>
    @Html.Partial(@MVC.Admin.Shared.Views._Header)
    <div class="content">
        @Html.Partial(@MVC.Admin.Shared.Views._SideBar)
        <div class="mainbar">
            @Html.Partial(@MVC.Admin.Shared.Views._PageHead)
            <div class="matter">
                <div class="container">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src="@Url.Content(@Links.Scripts.jquery_1_9_1_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.jquery_validate_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.jquery_validate_unobtrusive_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.additional_methods_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.jquery_unobtrusive_ajax_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.bootstrap.bootstrap_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.bootstrap.bootstrap_validation_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.angularjs.angular_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.angularjs.grid.ng_grid_2_0_7_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.slimscroll.jquery_slimscroll_min_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.padel.padel_js)" type="text/javascript"></script>
    <script src="@Url.Content(@Links.Scripts.padel.admin.admin_js)" type="text/javascript"></script>
    @RenderSection("JSSection")
</body>
</html>
